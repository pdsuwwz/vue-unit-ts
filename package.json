{
  "name": "vue-unit-ts",
  "description": "Unit Testing for Vue 3.x + TS",
  "version": "0.0.1",
  "author": "Wisdom <pdsu.wwz@foxmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc --noEmit --skipLibCheck && vite build",
    "preview": "vite preview --host",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx,.vue",
    "lint:fix": "eslint --fix --cache --ext .js,.ts,.jsx,.tsx,.vue .",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pdsuwwz/vue-unit-ts"
  },
  "bugs": {
    "url": "https://github.com/pdsuwwz/vue-unit-ts/issues"
  },
  "dependencies": {
    "vue": "^3.2.41"
  },
  "engines": {
    "node": ">=14.18.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "@sucrase/jest-plugin": "^3.0.0",
    "@types/jest": "^29.2.1",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/test-utils": "2.2.6",
    "@vue/vue3-jest": "^29.2.0",
    "babel-jest": "^29.2.2",
    "babel-plugin-transform-vite-meta-env": "^1.0.3",
    "eslint": "^8.26.0",
    "eslint-plugin-html": "7.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-vue": "^9.7.0",
    "jest": "^29.2.2",
    "jest-environment-jsdom": "29.3.1",
    "jest-transform-stub": "^2.0.0",
    "sucrase": "^3.28.0",
    "ts-jest": "29.0.3",
    "typescript": "^4.8.4",
    "vite": "^4.0.0",
    "vue-tsc": "1.0.17"
  }
}
